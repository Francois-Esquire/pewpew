{
  "name": "pewpew",
  "version": "0.1.0",
  "description": "shoot the sh*t",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Francois-Esquire/pewpew.git"
  },
  "keywords": [
    "pew",
    "pow",
    "poof",
    "plop"
  ],
  "author": "Michael Tobia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Francois-Esquire/pewpew/issues"
  },
  "homepage": "https://github.com/Francois-Esquire/pewpew#readme",
  "main": "index.js",
  "engines": {
    "node": "^8.2.1",
    "npm": "5.2.0"
  },
  "scripts": {
    "build": "NODE_ENV=production run-s clean build:render build:server webpack",
    "build:dll": "rimraf dist/dll && webpack --config internal/webpack.config.dll.js --display-optimization-bailout",
    "build:render": "rollup -c internal/rollup.config.render.js",
    "build:server": "rollup -c internal/rollup.config.server.js",
    "clean": "rimraf dist/public && rimraf dist/stats",
    "dev": "run-p watch start",
    "lint": "eslint . -c .eslintrc.js --ext .js --ext .jsx --ext .gql --ext .graphql",
    "start": "node index.js",
    "setup": "run-s build:dll build",
    "test": "mocha spec/*.test.js",
    "test:watch": "nodemon --exec 'npm run test'",
    "watch": "run-p watch:*",
    "watch:render": "rollup -c internal/rollup.config.render.js -w",
    "watch:server": "rollup -c internal/rollup.config.server.js -w",
    "webpack": "webpack --display-optimization-bailout"
  },
